{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CrewSchedulingOptimizer",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/CrewSchedulingOptimizer",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
